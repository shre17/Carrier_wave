<%= form_for :registration, url: registrations_path do |f| %>
<% if @registration.errors.any? %>
    <div id="error_explanation">
      <h2>
        <%= pluralize(@registration.errors.count, "error") %> prohibited
        this@registration from being saved:
      </h2>
      <ul>
        <% @registration.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <p>
    <%= f.label :Name%><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :Address%><br>
    <%= f.text_area :address %>
  </p>

  <p>
    <%= f.label :Phone%><br>
    <%= f.text_field :phone %>
  </p>

  <p>
    Gender<br />
    <%= f.radio_button :gender, 'Male' %> Male<br />
    <%= f.radio_button :gender, 'Female' %> Female<br />
</p>

  <p>
    <%= f.label :Email %><br>
    <%= f.email_field :email %>
  </p>

  <p>
    <%= f.label :Password%><br>
    <%= f.password_field :password %>
  </p>

  <p>
    <%= f.label :Profile_pic%><br>
    <%= f.file_field :profile_pic %>
  </p>
 
  <p>
    <%= f.submit %>

<%end%>